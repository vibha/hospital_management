<script type="text/javascript" src="http://iuhealth.org/utils/min/index.php?f=js/jquery.tools.min.js,js/jquery.easing.1.3.js,js/jquery.anchor.js,js/jquery.jqtransform.js,js/jquery.coda-slider-2.0.js,js/jquery.fancybox-1.3.4.pack.js,js/jquery.bgpos.js,js/twitStream.js,js/jquery.hoverIntent.minified.js,js/mega-drop.js"></script>

<script type="text/javascript">
// expand and collapse by class
$(document).ready(function () {

    //Hide (Collapse) the toggle containers on load
    $("#side-nav ul li ul.hide-sub").hide();
    $("#side-nav ul li ul.show-sub").show();

    //Switch the "Open" and "Close" state per click then slide up/down (depending on open/close state)
    $("#side-nav .btn").click(function () {
        $(this).toggleClass("alive").next().slideToggle("fast");
        return false; //Prevent the browser jump to the link anchor
    });
    $("#side-nav .active").click(function () {
        $(this).toggleClass("reverse").next().slideToggle("fast");
        return false; //Prevent the browser jump to the link anchor
    });

});
</script>
<script type="text/javascript">
var acEx = "<div class='accordionExpand'><a href='javascript: accordionExpand();'>Expand All</a>";
var acCo = "<div class='accordionCollapse'><a href='javascript: accordionCollapse();'>Collapse All</a>";

function aClick() {
	$(".accordion").find(".pane").css("display","none");
	$(".accordion").find("h2.current").next(".pane").css("display","block");
	$(".accordionExpand").show();
	$(".accordionCollapse").hide();
	$(".accordion h2").unbind('click', aClick);
}
function accordionExpand() {
	$(".accordion").find(".pane").css("display","block");
	$(".accordion h2").bind("click", aClick);
	$(".accordionExpand").before(acCo);
	$(".accordionExpand").hide();
}
function accordionCollapse() {
	$(".accordion").find(".pane").css("display","none");
	$(".accordion").find("h2.current").next(".pane").css("display","block");
	$(".accordionExpand").show();
	$(".accordionCollapse").hide();
}

$(function(){
	// Coda Slider
	
	if(typeof codaSliderInitComplete=='function'){codaSliderInitComplete();}
	$('#coda-slider-1').codaSlider({dynamicArrows:false,dynamicTabs:false,autoSlide:true,autoSlideInterval:6000,autoSlideStopWhenClicked:true,crossLinking:false});
    // FancyBox
	$("a.fancybox").fancybox({'overlayShow':true,'transitionIn':'elastic','transitionOut':'elastic'});
	$("a.iframe").fancybox({'width':'75%','height':'90%','autoScale': true,'transitionIn':'elastic','transitionOut':'elastic','type':'iframe'});
	$("a.inline").fancybox({'hideOnContentClick': false});
	// Tabbed Sections
    $("ul.tabs").tabs(".panes-wrapper > .panes", {effect: 'slide', speed: '4000'});
    $("ul.tabs-int").tabs(".panes-wrapper > .panes > .panes-int", {effect: 'slide', initialIndex: '0', speed: '4000'});
	// Accordion
	$(".accordion").tabs(".pane", {tabs:'h2',effect:'slide',initialIndex:'0'}).eq(0).before(acEx);
	// Contact Map
	$("#map-canvas").css("display","none");
	$("#map-canvas").fadeIn(2000);
	// Location Dropdown
	$("form.jqtransform").jqTransform();
	$(".jqTransformSelectWrapper").css({"width" : "262px"});
	$(".jqTransformSelectWrapper ul").css("width","260px");
	$(".quick-links ul li").eq(0).hide();
	$(".quick-links ul li").eq(5).addClass('five');
	$(".quick-links ul li").eq(6).addClass('six');
});

// Clear and Restore Search 
function clearText(s){if(s.defaultValue==s.value)s.value="";}
function resetText(s){if(s.value=='')s.value=s.defaultValue;}
function checkChanged(){
	if(document.getElementById('search-box').value=='Enter Search Term'){
		alert('Please Enter a Search Term');
		document.getElementById('search-box').focus();
		return false;
	}
}</script>







